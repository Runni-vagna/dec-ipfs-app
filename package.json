{
  "name": "cidfeed",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@10.2.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm dev:tauri",
    "dev:all": "turbo run dev --parallel",
    "dev:ui": "pnpm --filter @cidfeed/ui dev",
    "dev:tauri": "pnpm --filter @cidfeed/tauri dev",
    "build": "turbo run build",
    "build:ui": "pnpm --filter @cidfeed/ui build",
    "build:tauri": "pnpm --filter @cidfeed/tauri build",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "lint": "turbo run lint"
  },
  "devDependencies": {
    "turbo": "2.3.3",
    "typescript": "5.6.3"
  }
}
